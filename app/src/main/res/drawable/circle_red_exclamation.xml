<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval" xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#FFF95F62"/>
            <size android:height="311dp" android:width="311dp"/>
            <stroke android:width="1.5dp" android:color="#FF8492A6"/>
        </shape>
    </item>
    <item android:left="130dp" android:top="220dp" android:bottom="40dp" android:right="130dp">
        <shape android:shape="oval" xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#FFFFFFFF"/>
            <size android:height="0dp" android:width="40dp"/>
        </shape>
    </item>
    <item android:left="75dp" android:top="0dp" android:bottom="80dp" android:right="75dp">
        <rotate
            android:fromDegrees="90"
            android:toDegrees="90"
            android:pivotX="50%"
            android:pivotY="50%" >
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="line">
            <stroke
                android:width="30dp"
                android:color="#FFFFFF" />
            <size android:height="10dp" />
        </shape>
        </rotate>
    </item>
</layer-list>
